{
  "name": "tantor-core-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "yarn dev",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "extract-intl": "formatjs extract src/**/*.{js,jsx,ts,tsx} --ignore **/*.d.ts --out-file src/translations/es-mx.json --id-interpolation-pattern [sha512:contenthash:base64:6] --format simple",
    "extract-intl:unix": "formatjs extract 'src/**/!(*.d).{js,jsx,ts,tsx}' --out-file src/translations/es-mx.json --id-interpolation-pattern [sha512:contenthash:base64:6] --format simple",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@heroicons/react": "^2.1.1",
    "@nextui-org/react": "^2.2.9",
    "@types/apollo-upload-client": "^18.0.0",
    "apollo-upload-client": "^18.0.1",
    "axios": "1.1.3",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "framer-motion": "^10.18.0",
    "graphql": "^16.8.1",
    "jwt-decode": "^4.0.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.1",
    "react-router-dom": "5.3.3",
    "react-toastify": "^10.0.4",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.6",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
